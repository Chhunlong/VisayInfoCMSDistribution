{
    "name": "typo3cms/visay-info",
    "description": "TYPO3 CMS for visay.info website.",
    "keywords": ["visay-info", "typo3", "cms", "console"],
    "homepage": "http://www.visay.info",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Visay Keo",
            "email": "keo@visay.info",
            "homepage": "http://www.visay.info"
        }
    ],
    "config": {
        "vendor-dir": "/tmp/composer"
    },
    "require": {
        "php": ">=5.3.7",
        "composer/installers": "*",
        "typo3cms/realurl": "1.12.6",
        "typo3cms/news": "2.2.0",
        "typo3cms/templavoila": "1.8.0",
        "typo3cms/fal_dropbox": "*",
        "typo3cms/static_info_tables": "6.0.5",
        "typo3cms/formhandler": "dev-trunk",
        "typo3cms/sr_feuser_register": "3.0.2",
        "typo3cms/dropbox_carousel": "dev-master",
        "twitter/bootstrap": "2.2.2"
    },
    "require-dev": {
        "typo3cms/phpmyadmin": "dev-trunk",
        "typo3cms/extension_builder": "dev-master",
        "typo3cms/phpunit": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "typo3cms/realurl",
                "version": "1.12.6",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git://git.typo3.org/TYPO3v4/Extensions/realurl.git",
                    "type": "git",
                    "reference": "1_12_6"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/news",
                "version": "2.2.0",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git://git.typo3.org/TYPO3v4/Extensions/news.git",
                    "type": "git",
                    "reference": "2.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/templavoila",
                "version": "1.8.0",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git://git.typo3.org/TYPO3v4/Extensions/templavoila.git",
                    "type": "git",
                    "reference": "1_8_0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/fal_dropbox",
                "version": "master",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git@github.com:visay/fal_dropbox.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/static_info_tables",
                "version": "6.0.5",
                "type": "typo3-flow-package",
                "source": {
                    "url": "https://svn.typo3.org/TYPO3v4/Extensions/static_info_tables/tags/",
                    "type": "svn",
                    "reference": "Version-6-0-5"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/formhandler",
                "version": "trunk",
                "type": "typo3-flow-package",
                "source": {
                    "url": "https://svn.typo3.org/TYPO3v4/Extensions/formhandler/",
                    "type": "svn",
                    "reference": "trunk"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/sr_feuser_register",
                "version": "3.0.2",
                "type": "typo3-flow-package",
                "source": {
                    "url": "https://svn.typo3.org/TYPO3v4/Extensions/sr_feuser_register/tags/",
                    "type": "svn",
                    "reference": "Version-3-0-2"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/dropbox_carousel",
                "version": "master",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git@github.com:visay/dropbox_carousel.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "twitter/bootstrap",
                "version": "2.2.2",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git@github.com:twbs/bootstrap.git",
                    "type": "git",
                    "reference": "558bc52432840ea1b2f413438427164288f10350"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/phpmyadmin",
                "version": "trunk",
                "type": "typo3-flow-package",
                "source": {
                    "url": "https://svn.typo3.org/TYPO3v4/Extensions/phpmyadmin/",
                    "type": "svn",
                    "reference": "trunk"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/extension_builder",
                "version": "master",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git://git.typo3.org/TYPO3v4/Extensions/extension_builder.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "typo3cms/phpunit",
                "version": "master",
                "type": "typo3-flow-package",
                "source": {
                    "url": "git://git.typo3.org/TYPO3v4/Extensions/phpunit.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "extra": {
        "installer-paths": {
            "typo3conf/ext/{$name}/": [
                "typo3cms/realurl",
                "typo3cms/news",
                "typo3cms/templavoila",
                "typo3cms/fal_dropbox",
                "typo3cms/static_info_tables",
                "typo3cms/formhandler",
                "typo3cms/sr_feuser_register",
                "typo3cms/dropbox_carousel",
                "typo3cms/phpmyadmin",
                "typo3cms/extension_builder",
                "typo3cms/phpunit"
            ],
            "fileadmin/templates/{$name}/": [
                "twitter/bootstrap"
            ]
        }
    },
    "minimum-stability": "dev"
}